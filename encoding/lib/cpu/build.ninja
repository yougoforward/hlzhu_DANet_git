ninja_required_version = 1.3
cxx = c++

cflags = -DTORCH_EXTENSION_NAME=enclib_cpu -DTORCH_API_INCLUDE_EXTENSION_H -isystem /home/long/anaconda2/envs/pypy3/lib/python3.6/site-packages/torch/lib/include -isystem /home/long/anaconda2/envs/pypy3/lib/python3.6/site-packages/torch/lib/include/torch/csrc/api/include -isystem /home/long/anaconda2/envs/pypy3/lib/python3.6/site-packages/torch/lib/include/TH -isystem /home/long/anaconda2/envs/pypy3/lib/python3.6/site-packages/torch/lib/include/THC -isystem /home/long/anaconda2/envs/pypy3/include/python3.6m -fPIC -std=c++11
ldflags = -shared

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out
  depfile = $out.d
  deps = gcc

rule link
  command = $cxx $in $ldflags -o $out

build roi_align.o: compile /home/long/github/hlzhu_DANet/encoding/lib/cpu/roi_align.cpp
build roi_align_cpu.o: compile /home/long/github/hlzhu_DANet/encoding/lib/cpu/roi_align_cpu.cpp

build enclib_cpu.so: link roi_align.o roi_align_cpu.o

default enclib_cpu.so

